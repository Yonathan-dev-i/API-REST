# Example environment variables (do NOT commit your real .env file)
# Set your NewsAPI key here
NEWS_API_KEY=your_newsapi_key_here

# Optional: frontend proxy base (use if your frontend is on different origin than the server)
VITE_NEWS_PROXY_URL=http://localhost:4000

# TMDB proxy (optional, default points to local proxy)
VITE_TMDB_PROXY_URL=http://localhost:4000
TMDB_API_KEY=your_tmdb_api_key_here
